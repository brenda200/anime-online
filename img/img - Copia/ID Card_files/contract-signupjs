    if (val) { 
        $('#SocialMediaCheckboxSection').slideDown(); 
    } else {
        $("#SocialMediaCheckboxSection").hide(); 
      //  $('#previousHmpYes').slideUp();   
    }
}

function SecondCheckForDataPrivacyChangeHandler(e) {
    var checkBox = e.target;
    $(checkBox).valid();
    return false;
}

